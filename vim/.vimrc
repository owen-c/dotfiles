syntax enable
set number
set tabstop=4
set shiftwidth=4
set expandtab
set cindent
set incsearch
set laststatus=2

"let Tlist_Exit_OnlyWindow = 1
nmap <F5> :TlistToggle<CR>
set tags=./tags;
let Tlist_WinWidth = 50
let Tlist_GainFocus_On_ToggleOpen = 1
let Tlist_Show_One_File = 1
"match ErrorMsg '\%>80v.\+'

set nocp
filetype plugin on
